@model Domain.masa.Masa

@{
    ViewData["Title"] = "_Create";
}

<h1>Create New Masa</h1>

<h4>Masa</h4>
<hr />

<div class="row">
    <div class="col-md-4">
        <form asp-action="Create" method="post">
            <div class="form-group">
                <label asp-for="MasaNo" class="control-label"></label>
                <input asp-for="MasaNo" class="form-control" />
                <span asp-validation-for="MasaNo" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Kapasite" class="control-label"></label>
                <input asp-for="Kapasite" class="form-control" />
                <span asp-validation-for="Kapasite" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
